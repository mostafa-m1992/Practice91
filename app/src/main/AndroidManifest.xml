<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shia.practice91">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icwarning"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practice91">
        <activity android:name=".TvStore"></activity>
        <activity android:name=".ProfileStore" />
        <activity android:name=".FlowerStore" />
        <activity android:name=".FlagStore" />
        <activity android:name=".DressStore" />
        <activity android:name=".BikeStore" />
        <activity android:name=".CarStore" />
        <activity android:name=".BackPackStore" />
        <activity android:name=".LaptopStore" />
        <activity android:name=".PhoneStore" />
        <activity
            android:name=".Store"
            android:label="@string/store"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name=".SplashScrean"
            android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GettingInformation" />
        <activity android:name=".Information" />
        <activity android:name=".Car" />
        <activity
            android:name=".Messages"
            android:label="@string/messages">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
                <data android:mimeType="video/*" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GettingRegistration"
            android:label="@string/gettingRegistration" />
        <activity
            android:name=".Registration"
            android:label="@string/registration" />
        <activity
            android:name=".Counter"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/counter" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>